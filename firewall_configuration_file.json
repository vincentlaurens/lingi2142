{
	"Halles": {
		"conf_firewall":[
			"ip6tables -A INPUT -i $Halles-eth1 -p tcp --dport 22 -j ACCEPT",
			"# Restrict incoming SSH to the local network",
			"ip6tables -A INPUT -i $Halles-eth1 -p tcp -s fd00:300:3::1/48 --dport 22 -j ACCEPT",
			"ip6tables -A INPUT -i $Halles-eth1 -p tcp -s fd00:200:3::1/48 --dport 22 -j ACCEPT"
		]
